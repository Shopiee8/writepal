<script defer type="2e440af07986c993aee0f0af-text/javascript">
  
  jQuery(document).ready(function ($) {
    $(function () {
      $url = window.location.href;
      $urlSplit = $url.split("?");
      $url_parameters = $urlSplit[1];
      $("a").attr('href', function (i, h) {
