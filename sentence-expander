<script defer type="38f2761231968fb79e3add15-text/javascript">
  
  jQuery(document).ready(function ($) {
    $(function () {
      $url = window.location.href;
      $urlSplit = $url.split("?");
      $url_parameters = $urlSplit[1];
      $("a").attr('href', function (i, h) {
