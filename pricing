</script><script type="3eb44a0086679ccfdcdc8dd8-text/javascript">
  var Webflow = Webflow || [];
  Webflow.push(function () {
    // DOMready has fired
    // May now use jQuery and Webflow api

    $('[data-name=select').click(function(){
      if ($('[data-block=select-item]').is(':hidden')) 
        $('[data-block=select-item]').show();
      else
        $('[data-block=select-item]').hide();
    });
    $('[data-name=select-item').click(function(){
      $('[data-block=select-item]').hide();
      $('[data-name=select]').find('.select-item').text($(this).find('.select-item').text());
      $(this).each(function() {
        $.each(this.attributes, function() {
          if(this.specified) {
            //if (this.name=='data-name') continue;     
            $('[data-id='+this.name+']').text(this.val());
          }
        });
      });
    });

    let longIndex = 0;
    let qualityIndex = 'premium';
    let subscriptionType = 'priceAnnually';
    
  	let premiumL =  [100000, 300000, 600000, 1200000, 3000000, 6000000];
    let superiorL = [33334, 100000, 200000, 400000, 1000000, 2000000];
    
    const dataLongForm = {
      priceMonthly: [19, 49, 99, 199, 499, 999],
      priceAnnually: [12.67, 32.67, 66, 132.67, 332.67, 666],
      seats: [1, 5, 5, 10, 12, 15],
      premium: premiumL,
      superior:superiorL
    }

    
    function reRender(){
      $("#longMonthly").text("$"+dataLongForm[subscriptionType][longIndex])
      $("#longWords").text(String(dataLongForm[qualityIndex][longIndex]).replace(/(?!^)(?=(?:\d{3})+(?:\.|$))/gm, ',') +" words")
      $("#longSeats").text(dataLongForm.seats[longIndex]+" users")
      $('#qualityTabList > p').removeClass('active')
      $('#qualityTab-'+qualityIndex).addClass('active')
      $("#longSlider .sliderMin").text(String(dataLongForm[qualityIndex][0]).replace(/(?!^)(?=(?:\d{3})+(?:\.|$))/gm, ','))
      $("#longSlider .sliderMax").text(String(dataLongForm[qualityIndex][5]).replace(/(?!^)(?=(?:\d{3})+(?:\.|$))/gm, ','))
      
      
      if(dataLongForm.seats[longIndex] === 1){
      	$("#longSeats").text(dataLongForm.seats[longIndex]+" user");
      } else {
      	$("#longSeats").text(dataLongForm.seats[longIndex]+" users");
      }
      
    }
    
    reRender();

    $('#qualityTabList > p').on('click', function () {
      qualityIndex = ['premium', 'superior'][$('#qualityTabList > p').index(this)]
      reRender()
    })

    $('#subscriptionType').on('click', function () {
      subscriptionType = subscriptionType === 'priceMonthly' ? 'priceAnnually' : 'priceMonthly';
      reRender();
    })
    let currentPoint;
    function Slider(slider, defaultValue, onChange) {

      this.slider = slider;
      this.factor = 100 / (slider.find('.slider-spec-pos').length - 1);

      this.slider.find('.slider-input').val(defaultValue * this.factor)

      const inputFunction = (function() {
        this.updateSliderOutput();
        this.updateSliderLevel();
        this.updateBars();
        this.slider.find('.slider-spec-pos').removeClass('active');
        currentPoint = Math.round(+this.level()/this.factor) + 1;
        for(let i = 1; i <= currentPoint; i++){
          this.slider.find('.pos'+i).addClass('active')
        }
        onChange && onChange(Math.round(+this.level()/this.factor) + 1)
      }).bind(this)


      const changeFunction = (function() {
        let currentValue = this.level()
        const destValue = Math.round(+this.level()/this.factor) * this.factor
        const steps = 40
        let curStep = 0
        const stepValue = (destValue-currentValue)/steps
        if ( this.timer ) clearInterval(this.timer)
        this.timer = setInterval(
          () => {
            currentValue = +currentValue+stepValue
            this.slider.find('.slider-input').val(currentValue)
            inputFunction()
            curStep += 1

            if (curStep>=steps) {
              this.timer && clearInterval(this.timer)
              this.timer = null
            }
          }, 30/steps
        )
        inputFunction()
        onChange && currentPoint && onChange(currentPoint)
      }).bind(this)

      slider.on('input', inputFunction);
      slider.on('change', changeFunction);
      this.level = function() {
        // actual slider
        let level = this.slider.find('.slider-input');
        console.log(level)
        return level.val();
      }

      this.levelString = function() {
        return parseFloat(this.level());
      }

      this.updateSliderOutput = function() {
        // number indicator below slider
        // let output = this.slider.find('.slider-output');
        // slider circle handle
        let thumb = this.slider.find('.slider-thumb');
        // output.val() = this.levelString();
        // output.style.left = this.levelString() + '%';
        thumb.css('left',this.levelString() + '%');
      }

      this.updateSliderLevel = function() {
        // dark gray part of slider
        let level = this.slider.find('.slider-level');
        level.css("width", this.levelString() + '%')
      }

      this.updateBars = function() {
        // actual slider
        let level = this.slider.find('.slider-input');

        let analogousLevel = level.val();
        let inverseLevel = 100 - level.val();
        $('.fill_analogous').css('width', analogousLevel + '%');
        $('.fill_inverse').css('width', inverseLevel + '%');
      }
      changeFunction()
    }

    let longSlider = $('#longSlider');

    new Slider(longSlider, longIndex, (newLongIndex) => {
      longIndex = newLongIndex - 1;
      reRender()
    });
  });
</script><script src="cdn-cgi/scripts/7d0fa10a/cloudflare-static/rocket-loader.min.js" data-cf-settings="3eb44a0086679ccfdcdc8dd8-|49" defer=""></script></body></html>